<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".findforpay.FindPayMainActivity">

    <com.example.framework.view.MyToorbar
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        app:isleft="true"
        app:leftimg="@drawable/top_bar_left_back"
        app:titletext="@string/payfor"></com.example.framework.view.MyToorbar>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rv"></androidx.recyclerview.widget.RecyclerView>
</LinearLayout>